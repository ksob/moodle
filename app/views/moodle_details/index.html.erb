<p id="notice"><%= notice %></p>

<h1>Moodle Details</h1>

<table>
  <thead>
    <tr>
      <th>Host</th>
      <th>Token</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @moodle_details.each do |moodle_detail| %>
      <tr>
        <td><%= moodle_detail.host %></td>
        <td><%= moodle_detail.token %></td>
        <td><%= moodle_detail.user %></td>
        <td><%= link_to 'Show', moodle_detail %></td>
        <td><%= link_to 'Edit', edit_moodle_detail_path(moodle_detail) %></td>
        <td><%= link_to 'Destroy', moodle_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Moodle Detail', new_moodle_detail_path %>
